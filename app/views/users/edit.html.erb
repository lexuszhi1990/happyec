<%# title "Edit Profile" %>

<div class="content">
<%= form_for @user do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </p>
  <p><%= f.submit "Update Profile" %></p>
<% end %>
</div>
